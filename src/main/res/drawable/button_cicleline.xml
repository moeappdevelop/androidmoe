<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval" >
            <stroke android:color="@color/moepink" android:width="0.8dp">

            </stroke>
            <size android:width="75dp" android:height="75dp">

            </size>
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp"
                />

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval" >
            <stroke
                android:color="@color/moepink" android:width="2dp">
            </stroke>
            <size android:width="75dp" android:height="75dp">

            </size>
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp"
                />

        </shape>
</item>
</selector>
